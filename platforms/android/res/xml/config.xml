<?xml version='1.0' encoding='utf-8'?>
<widget id="com.azuredog.dog" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="AdmobPlugin">
        <param name="android-package" value="com.admob.cordova.plugin.AdmobAdPlugin" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.admob.AdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="connectivity">
        <param name="android-package" value="com.appfeel.cordova.connectivity.Connectivity" />
    </feature>
    <icon src="icon.png" />
    <preference name="orientation" value="landscape" />
    <name>Pooch</name>
    <description>
        DORG!
    </description>
    <author email="lucasdragonia@gmail.com">
        Azure Dog Team    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
